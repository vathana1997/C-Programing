#include <stdio.h>

// Function to find maximum
int findMaximum(int a, int b) {
    if (a > b)
        return a;
    else
        return b;
}

int main() {
    int num1, num2, max;

    // Input from user
    printf("Enter first number: ");
    scanf("%d", &num1);

    printf("Enter second number: ");
    scanf("%d", &num2);

    // Call the function
    max = findMaximum(num1, num2);

    // Display result
    printf("Maximum = %d\n", max);

    return 0;
}
