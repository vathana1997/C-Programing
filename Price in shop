#include <stdio.h>

int main() {
    int n, i, found = 0;
    float price, items[20];

    printf("Enter number of items: ");
    scanf("%d", &n);

    for (i = 0; i < n; i++) {
        printf("Enter price of item %d: ", i + 1);
        scanf("%f", &items[i]);
    }

    printf("Enter price to search: ");
    scanf("%f", &price);

    for (i = 0; i < n; i++) {
        if (items[i] == price) {
            found = 1;
            break;
        }
    }

    if (found)
        printf("Item found");
    else
        printf("Item not found");

    return 0;
}
