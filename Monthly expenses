#include <stdio.h>

int main() {
    float rent, food, travel, others, total;

    printf("Enter rent expense: ");
    scanf("%f", &rent);
    printf("Enter food expense: ");
    scanf("%f", &food);
    printf("Enter travel expense: ");
    scanf("%f", &travel);
    printf("Enter other expenses: ");
    scanf("%f", &others);

    total = rent + food + travel + others;
    printf("Total Monthly Expense = %.2f\n", total);

    return 0;
}
