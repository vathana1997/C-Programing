#include <stdio.h>

int main() {
    int a[10], b[10], c[20];
    int n1, n2, i, j = 0;

    printf("Enter size of first array: ");
    scanf("%d", &n1);

    for (i = 0; i < n1; i++)
        scanf("%d", &a[i]);

    printf("Enter size of second array: ");
    scanf("%d", &n2);

    for (i = 0; i < n2; i++)
        scanf("%d", &b[i]);

    for (i = 0; i < n1; i++)
        c[j++] = a[i];

    for (i = 0; i < n2; i++)
        c[j++] = b[i];

    printf("Merged array:\n");
    for (i = 0; i < n1 + n2; i++)
        printf("%d ", c[i]);

    return 0;
}
