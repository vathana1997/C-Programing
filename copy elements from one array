#include <stdio.h>

int main() {
    int n, i;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr1[n], arr2[n];
    printf("Enter %d elements:\n", n);
    for (i = 0; i < n; i++) {
        scanf("%d", &arr1[i]);
        arr2[i] = arr1[i]; // copy
    }

    printf("\nOriginal Array: ");
    for (i = 0; i < n; i++) printf("%d ", arr1[i]);

    printf("\nCopied Array: ");
    for (i = 0; i < n; i++) printf("%d ", arr2[i]);

    return 0;
}
